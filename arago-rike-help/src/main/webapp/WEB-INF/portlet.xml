<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	<portlet>
		<description>Help</description>
		<portlet-name>rike-help</portlet-name>
		<display-name>Help</display-name>
		<portlet-class>de.arago.rike.help.Help</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
    
    <portlet-info>
			<title>Help</title>
			<short-title>Help</short-title>
			<keywords></keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>portlet-setup-show-borders</name>
				<value>false</value>
			</preference>
			<preference>
				<name>portlet-setup-hide-topper</name>
				<value>true</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>


	

</portlet-app>
