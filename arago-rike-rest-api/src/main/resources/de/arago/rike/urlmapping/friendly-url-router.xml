<?xml version="1.0"?>  
    <!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">  
      
<routes>  
  <route>  
    <pattern>/task/{id}</pattern>  
    <implicit-parameter name="p_p_id">aragoriketask_WAR_aragorike</implicit-parameter>  
    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>  
    <implicit-parameter name="p_p_state">normal</implicit-parameter>  
    <implicit-parameter name="p_p_mode">view</implicit-parameter>  
    <implicit-parameter name="action">selectTask</implicit-parameter>  
  </route>  
  <route>  
    <pattern>/milestone/{id}</pattern>  
    <implicit-parameter name="p_p_id">aragoriketask_WAR_aragorike</implicit-parameter>  
    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>  
    <implicit-parameter name="p_p_state">normal</implicit-parameter>  
    <implicit-parameter name="p_p_mode">view</implicit-parameter>  
    <implicit-parameter name="action">selectMilestone</implicit-parameter>  
  </route>  
  <route>  
    <pattern>/artifact/{id}</pattern>  
    <implicit-parameter name="p_p_id">aragoriketask_WAR_aragorike</implicit-parameter>  
    <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>  
    <implicit-parameter name="p_p_state">normal</implicit-parameter>  
    <implicit-parameter name="p_p_mode">view</implicit-parameter>  
    <implicit-parameter name="action">selectArtifact</implicit-parameter>  
  </route>  
</routes>
